:root {
    --primary-color: #007ba7;
    --secondary-color: #4dc1ba;
    --white-clr: rgb(255, 255, 255);
    --black-clr: #000;
    --body-clr: #333;
    --grey: #F9F9F9;
    --grey2: #999999;
    --dark-grey: #757575;
}

html,
body {
    height: 100%;
}

html {
    font: 400 16px/1.5 'Poppins', sans-serif;
    scroll-behavior: smooth;
}

body {
    margin: 0;
    scroll-behavior: smooth;
    -webkit-overflow-scrolling: touch;
    font: 400 18px/1.6 'Poppins', sans-serif;
    color: rgba(var(--body-clr), .8);
    width: 100%;
    background-color: var(--white-clr);
}

ul,
li,
ol {
    list-style: none;
    padding: 0;
    margin: 0;
}

a {
    outline: 0;
    text-decoration: none;
    color: inherit;
    cursor: pointer;
    -webkit-transition: all 0.2s ease;
    -moz-transition: all 0.2s ease;
    -ms-transition: all 0.2s ease;
    -o-transition: all 0.2s ease;
    transition: all 0.2s ease;
}

a:active,
a:focus,
a:hover {
    outline: 0;
    text-decoration: none;
}

button:active,
button:focus {
    outline: 0 !important;
    text-decoration: none;
}

input:focus,
select:focus {
    box-shadow: none !important;
}

input:focus,
select:focus {
    border: 2px solid var(--primary-color) !important;
}

.btn.focus,
.btn:focus {
    box-shadow: none !important;
    border-color: none !important;
}

:focus {
    outline: 0 !important;
}

.form-check-input:checked {
    background-color: var(--primary-color);
    border-color: var(--primary-color);
}

header.custom-header {
    position: absolute;
    width: 100%;
    top: 0;
    z-index: 3;
}

header.sticky-header {
    position: fixed;
    background: rgb(255 255 255 / 80%);
    animation: smoothScroll 1s forwards;
}

@keyframes smoothScroll {
    0% {
        transform: translateY(-40px);
    }
    100% {
        transform: translateY(0px);
    }
}

.notification span {
    font-size: 9px;
    background-color: #E7121F;
    color: #fff;
    border-radius: 100%;
    width: 12px;
    height: 12px;
    display: flex;
    align-items: center;
    justify-content: center;
    top: 0px;
    right: -2px;
}

.custom-header ul.navbar-nav li a {
    color: #707070;
    text-transform: uppercase;
    letter-spacing: 1px;
    font-weight: 500;
    font-size: 16px;
    color: #707070;
    padding: 0 20px !important;
    -webkit-transition: all 0.2s ease;
    -moz-transition: all 0.2s ease;
    -ms-transition: all 0.2s ease;
    -o-transition: all 0.2s ease;
    transition: all 0.2s ease;
}

.custom-header ul li a:hover,
.custom-header ul li a:focus {
    -webkit-transform: scale(1.1);
    -moz-transform: scale(1.1);
    transform: scale(1.1);
    color: var(--primary-color) !important;
    font-weight: 600;
}

.user-setting {
    top: 52px !important;
    border-radius: 15px;
    filter: drop-shadow(0px 3px 12px rgba(0, 0, 0, 0.10));
    border: 0;
    min-width: 14rem;
}

.user-setting:after {
    position: absolute;
    content: '';
    width: 0;
    height: 0;
    border-left: 10px solid transparent;
    border-right: 10px solid transparent;
    border-bottom: 15px solid #fff;
    top: -15px;
    right: 15px;
    filter: drop-shadow(0px 0px 0 rgba(0, 0, 0, 0.19));
}

.user-setting li:hover {
    border-color: var(--primary-color) !important;
}

.user-setting li:hover a {
    color: var(--primary-color) !important;
}

.user-setting li:hover svg path {
    fill: var(--primary-color);
}

.user-setting a:hover {
    transform: none !important;
    background-color: transparent;
    font-weight: normal !important;
}

.default-btn,
.default-btn-2:hover {
    border-radius: 4px;
    background: var(--primary-color);
    color: var(--white-clr);
    padding: 10px 30px;
    margin-right: 10px;
    border: 1px solid transparent;
    cursor: pointer;
}

.default-btn:hover {
    background: var(--white-clr);
    color: var(--primary-color);
    border: 1px solid var(--primary-color);
}

.default-btn-2 {
    box-shadow: 0px 3px 10px rgba(0, 0, 0, 0.06);
