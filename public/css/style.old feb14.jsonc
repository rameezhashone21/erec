/*=====================media=========================*/

@media (min-width: 1400px) {
    .banner-content h1,
    .socialism h2,
    .daily-progress h2,
    .progress-box h2,
    .candidates h2,
    .employers h2,
    .recruiters h2 {
        font-size: 48px;
    }
}

@media screen and (max-width: 1366px) {
    .bg-circle .img {
        width: 76px;
        height: 76px;
    }
    .bg-circle .img.one {
        left: calc(50% - 48px);
        top: -35px;
    }
    .bg-circle .dot.one {
        left: 46px;
    }
    .bg-circle .dot.two {
        left: 120px;
        bottom: 20px;
    }
    .bg-circle .btn.candidates {
        top: calc(50% - 60px);
    }
}

@media screen and (max-width: 768px) {
    .select-main .logo-center {
        top: 10%;
        left: 0;
        right: 0;
        width: 100%;
        transform: unset;
        text-align: center;
    }
    .select-main .bg-circle {
        top: 15%;
        width: 70vh;
        height: 70vh;
    }
    .bg-circle .dot.one {
        left: 35px;
    }
    .bg-circle .btn.employers {
        right: 52px;
    }
    .bg-circle .dot.two {
        bottom: 10px;
    }
    .bg-circle .btn.recruiters {
        right: -30px;
    }
    .p-x-70 {
        padding-left: 25px;
        padding-right: 25px;
    }
    .sign-up ul li a:after,
    .sign-in ul li a:after {
        display: none;
    }
    .fs-48 {
        font-size: 28px;
    }
    .details li.d-lg-flex div:first-child,
    .details li.d-lg-flex div:last-child {
        width: 100%;
    }
}

@media screen and (max-width: 480px) {
    .select-main .bg-circle {
        left: 180px;
        width: 100vh;
        height: 100vh;
        top: 5%;
        animation: unset;
    }
    .bg-circle .btn {
        animation: unset;
    }
    .bg-circle .img.two,
    .bg-circle .img.one,
    .bg-circle .img.three {
        display: none;
    }
    .bg-circle .btn.recruiters {
        right: unset;
    }
    .bg-circle .btn.employers {
        right: unset;
        bottom: 170px;
    }
    .bg-circle .btn.candidates {
        top: calc(50% - 70px);
    }
    .bg-circle .dot.one {
        top: 28%;
        left: 25px;
    }
    .bg-circle .dot.two {
        bottom: 270px;
        left: 0;
    }
}

@media screen and (max-width: 375px) {
    .select-main .bg-circle {
        left: 130px;
    }
}
