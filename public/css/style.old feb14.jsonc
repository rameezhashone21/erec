}

.sign-up ul li:nth-child(3):hover a:after {
    content: 'Sign up with Twitter';
}

.sign-in ul li:nth-child(1):hover a:after {
    content: 'Sign in with Google';
}

.sign-in ul li:nth-child(2):hover a:after {
    content: 'Sign in with Linkedin';
}

.sign-in ul li:nth-child(3):hover a:after {
    content: 'Sign in with Twitter';
}

.sign-up ul li:hover a:after,
.sign-in ul li:hover a:after {
