/*================Step Form Style===================*/

#msform {
    position: relative;
    margin-top: 30px;
}

#msform fieldset {
    background: white;
    border: 0 none;
    border-radius: 16px;
    box-shadow: 0px 3px 20px rgba(0, 0, 0, 0.06);
    padding: 20px 30px;
    box-sizing: border-box;
    width: 65%;
    margin: 0 auto;
    position: relative;
}

#msform input[type="text"],
#msform input[type="email"],
#msform input[type="tel"],
#msform input[type="date"],
#msform select,
#msform textarea {
    padding: 15px;
    margin-bottom: 10px;
    width: 100%;
    box-sizing: border-box;
    border: 2px solid var(--dark-grey);
}

#msform label.btn-outline-primary {
    background: var(--grey) !important;
    color: var(--black-clr);
    border-color: var(--grey) !important;
    border-radius: 50px;
    padding: 10px 20px;
}

#msform .btn-check:checked+.label.radio,
#msform .btn-check:active+.btn-outline-primary,
#msform .btn-check:checked+.btn-outline-primary,
#msform .btn-outline-primary.active,
#msform .btn-outline-primary.dropdown-toggle.show,
#msform .btn-outline-primary:active {
    border: 2px solid var(--primary-color) !important;
    box-shadow: none;
}

#msform input[type="text"]:focus,
#msform input[type="email"]:focus,
#msform input[type="tel"]:focus,
#msform input[type="date"]:focus,
#msform select:focus #msform textarea:focus {
    -moz-box-shadow: none !important;
    -webkit-box-shadow: none !important;
    box-shadow: none !important;
    outline-width: 0;
    transition: All 0.5s ease-in;
    -webkit-transition: All 0.5s ease-in;
    -moz-transition: All 0.5s ease-in;
    -o-transition: All 0.5s ease-in;
    background-color: var(--grey);
}

#msform .action-button-previous {
    width: 100px;
    background: #C5C5F1;
    font-weight: bold;
    color: white;
    border: 0 none;
    border-radius: 25px;
    cursor: pointer;
    padding: 10px 5px;
    margin: 10px 5px;
}

#msform .action-button-previous:hover,
#msform .action-button-previous:focus {
    box-shadow: 0 0 0 2px white, 0 0 0 3px #C5C5F1;
}

.fs-title {
    font-size: 18px;
    text-transform: uppercase;
    color: #2C3E50;
    margin-bottom: 10px;
    letter-spacing: 2px;
    font-weight: bold;
}

#progressbar {
    margin-bottom: 30px;
    overflow: hidden;
    counter-reset: step;
}

#progressbar li {
    list-style-type: none;
    color: white;
    text-transform: uppercase;
    font-size: 9px;
    width: 33.33%;
    float: left;
    position: relative;
    letter-spacing: 1px;
}

#progressbar li:before {
    content: counter(step);
    counter-increment: step;
    width: 60px;
    height: 40px;
    line-height: 26px;
    display: block;
    font-size: 12px;
    color: #fff;
    background: white;
    border-radius: 25px;
    margin: 0 auto 10px auto;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 24px;
    background: #C9C9C9;
}

#progressbar li:after {
    content: '';
    width: 100%;
    height: 2px;
    background: #C9C9C9;
    position: absolute;
    left: -50%;
    top: 18px;
    z-index: -1;
}

#progressbar li:first-child:after {
    content: none;
}

#progressbar li.active:before,
#progressbar li.active:after {
    background: var(--primary-color);
}

.cut {
    background: var(--grey) !important;
    color: var(--black-clr);
    border-color: var(--grey) !important;
    border-radius: 50px;
    padding: 10px 20px;
}


/*================Step Form Style END===================*/


/*================Profile Upload =======================*/

.avatar-upload {
    position: relative;
}

.avatar-upload .avatar-edit {
    position: absolute;
    z-index: 1;
}

.avatar-upload .avatar-edit input {
    display: none;
}

.avatar-upload .avatar-edit label {
    width: 120px;
    height: 120px;
    border-radius: 100%;
    background: transparent;
    cursor: pointer;
    transition: all .2s ease-in-out;
}

.avatar-preview {
    width: 120px;
    height: 120px;
    position: relative;
}

.avatar-upload .avatar-preview div {
    width: 100%;
    height: 100%;
    border-radius: 100%;
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center;
}


/*================Profile Upload END=======================*/


/*================docx Upload =======================*/

#label input {
    display: none;
}

#label {
    padding: 30px;
    border: 2px dashed #757575;
    border-radius: 5px;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    gap: 10px;
    cursor: pointer;
    background-color: #EFF7FE;
}

#label span {
    max-width: 100%;
    font-family: sans-serif;
    text-align: center;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}


/*================docx Upload END=======================*/


/*==============Job Listing Page CSS==============*/

.search-area {
    border-radius: 15px;
    box-shadow: 0px 3px 20px rgba(0, 0, 0, 0.06);
    /* filter: drop-shadow(0px 3px 20px rgba(0, 0, 0, 0.06)); */
}

.search-area input {
    border-radius: 4px;
    border: 2px solid var(--primary-color);
    color: var(--black-clr);
}

.search-area img {
    left: 45px;
}

.search-area .col:first-child img {
    top: 40px;
}

.search-area .col:last-child img {
    top: 105px;
}

.border-bottom-dash {
    border-bottom: 1px solid var(--grey2);
    border-bottom-style: dashed;
}

.horizontal-view {
    box-shadow: 0px 3px 20px rgb(0 0 0 / 6%);
    border-radius: 15px;
}

.details .title {
    font-size: 23px;
}

#pointer-alt {
    padding: 5px 5px;
    position: relative;
    background: #D2EAD5;
    font-size: 17px;
    color: #00A716;
    font-weight: 600;
    margin-right: 23px;
}

#pointer-alt:after {
    position: absolute;
    content: '';
    width: 0;
    height: 0;
    border-top: 18px solid transparent;
    border-left: 20px solid #d2ead5;
    border-bottom: 18px solid transparent;
    top: -1px;
    right: -19px;
}

#pointer-alt:before {
    position: absolute;
    content: "";
    right: -10px;
    top: 12px;
    z-index: 9;
    border-radius: 50%;
    width: 8px;
    height: 8px;
    background-color: #00a716;
}

.details li {
    padding: 5px 0;
}

.details li.d-lg-flex div:first-child {
    width: 30%;
    font-weight: 600;
    position: relative;
}

.details li.d-lg-flex div:first-child:after {
    position: absolute;
    content: ':';
    right: 10px;
    color: #000;
    font-size: 15px;
    top: 3px;
    font-weight: bold;
}

.details li.d-lg-flex div:last-child {
    width: 70%;
}


/*==============Job Listing Page CSS END==============*/


/*==============Job Listing Page CSS Details==============*/

.rate {
    border-bottom-right-radius: 12px;
    border-bottom-left-radius: 12px
}

.rating {
    display: flex;
    flex-direction: row-reverse;
    justify-content: start;
    align-items: center;
}

.rating>input {
    display: none
}

.rating>label {
    position: relative;
    width: 0.8em;
    font-size: 20px;
    font-weight: 300;
    color: var(--primary-color);
    cursor: pointer
}

.rating>label::before {
    content: "\2605";
    position: absolute;
    opacity: 0
}

.rating>label:hover:before,
.rating>label:hover~label:before {
    opacity: 1 !important
}

.rating>input:checked~label:before {
    opacity: 1
}

.rating:hover>input:checked~label:before {
    opacity: 0.4
}

.buttons {
    top: 36px;
    position: relative
}

.rating-submit {
    border-radius: 15px;
    color: #fff;
    height: 49px
}

.rating-submit:hover {
    color: #fff
}

.services .card {
    border-radius: 16px;
    position: relative;
    margin-bottom: 30px;
}

.services .card:before {
    position: absolute;
    content: '';
    border-radius: 10px;
    width: 20px;
    height: 77px;
    background-color: var(--primary-color);
    left: 0;
}

.social-share li {
    margin: 0 0 0 10px;
}

a.lnkd {
    background: #346da6;
    filter: drop-shadow(0px 3px 10px rgba(0, 0, 0, 0.06));
    padding: 10px 25px;
    border-radius: 5px;
    color: #fff;
}

a.twt {
    background: #00dfef;
    box-shadow: 0px 3px 10px rgba(0, 0, 0, 0.06);
    padding: 10px 30px;
    border-radius: 5px;
    color: #fff;
}

a.fb {
    border-radius: 4px;
    background: #3a559f;
    box-shadow: 0px 3px 10px rgba(0, 0, 0, 0.06);
    padding: 10px 30px;
    color: #fff;
}


