#intro-svg {
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    width: auto;
    height: 80%;
}

.svg-rotate {
    animation: rotation 40s infinite linear;
}

.svg-rotate:hover {
    -webkit-animation-play-state: paused;
    animation-play-state: paused;
}

@keyframes rotation {
    from {
        transform: rotate(0deg);
    }
    to {
        transform: rotate(359deg);
    }
}


/* buble animation */

.bubbles {
    z-index: -1;
    overflow: hidden;
    width: 100%;
    position: absolute;
    height: 100%;
}

.rat-rect {
    position: absolute;
    left: 0;
    bottom: 40%;
    -webkit-animation: animationFramesOne 15s infinite linear;
    animation: animationFramesOne 15s infinite linear;
    width: 110px;
}

.rat-rect-2 {
    position: absolute;
    right: 50%;
    top: 15%;
    -webkit-animation: animationFramesOne 30s infinite linear;
    animation: animationFramesOne 30s infinite linear;
    position: absolute;
    width: 35px;
}

.rat-rect-3 {
    -webkit-animation: animationFramesOne 50s infinite linear;
    animation: animationFramesOne 50s infinite linear;
    position: absolute;
    right: 65%;
    top: 65%;
    width: 60px;
}

.rat-rect-4 {
    -webkit-animation: animationFramesOne 40s infinite linear;
    animation: animationFramesOne 40s infinite linear;
    position: absolute;
    right: 0%;
    top: 25%;
    width: 90px;
}

.rat-rect-5 {
    -webkit-animation: animationFramesOne 60s infinite linear;
    animation: animationFramesOne 60s infinite linear;
    position: absolute;
    right: 5%;
    bottom: 15%;
    width: 70px;
}

@-webkit-keyframes animationFramesOne {
    0% {
        -webkit-transform: translate(0px, 0px) rotate(0deg);
        transform: translate(0px, 0px) rotate(0deg);
    }
    20% {
        -webkit-transform: translate(73px, -1px) rotate(36deg);
        transform: translate(73px, -1px) rotate(36deg);
    }
    40% {
        -webkit-transform: translate(141px, 72px) rotate(72deg);
        transform: translate(141px, 72px) rotate(72deg);
    }
    60% {
        -webkit-transform: translate(83px, 122px) rotate(108deg);
        transform: translate(83px, 122px) rotate(108deg);
    }
    80% {
        -webkit-transform: translate(-40px, 72px) rotate(144deg);
        transform: translate(-40px, 72px) rotate(144deg);
    }
    100% {
        -webkit-transform: translate(0px, 0px) rotate(0deg);
        transform: translate(0px, 0px) rotate(0deg);
    }
}

@keyframes animationFramesOne {
    0% {
        -webkit-transform: translate(0px, 0px) rotate(0deg);
        transform: translate(0px, 0px) rotate(0deg);
    }
    20% {
        -webkit-transform: translate(73px, -1px) rotate(36deg);
        transform: translate(73px, -1px) rotate(36deg);
    }
    40% {
        -webkit-transform: translate(141px, 72px) rotate(72deg);
        transform: translate(141px, 72px) rotate(72deg);
    }
    60% {
        -webkit-transform: translate(83px, 122px) rotate(108deg);
        transform: translate(83px, 122px) rotate(108deg);
    }
    80% {
        -webkit-transform: translate(-40px, 72px) rotate(144deg);
        transform: translate(-40px, 72px) rotate(144deg);
    }
    100% {
        -webkit-transform: translate(0px, 0px) rotate(0deg);
        transform: translate(0px, 0px) rotate(0deg);
    }
}


/* bubble animation */

.select-main {
    width: 100%;
    height: 100%;
    padding: 10vh 0 0;
    overflow: hidden;
}

.select-main .logo-center {
    position: absolute;
    top: 50%;
    left: calc(50% + 24px);
    transform: translate(-50%, -50%);
    width: 160px;
    height: 37px;
}

.select-main .logo-center:after {
    content: '';
    position: absolute;
    left: -36px;
    width: 24px;
    height: 24px;
    top: 50%;
    transform: translateY(-50%);
    background: url(../images/arrow-left.png) center 100%;
}

.select-main .bg-circle {
    margin: 0 auto;
    width: 80vh;
    height: 80vh;
    background-image: url(../images/bg-dash-circle.png);
    background-size: 100%;
    background-position: center;
    position: relative;
    animation: mainRotate 40s linear infinite;
}

.bg-circle .btn {
    border-radius: 24px;
    position: absolute;
    font-family: Montserrat-Bold, Montserrat;
    font-size: 18px;
    font-weight: 700;
    text-align: center;
    color: #ffffff;
    padding: 10px 25px;
    cursor: pointer;
    z-index: 1;
    transition: background-color 200ms linear;
    animation: rotateInner 40s linear infinite;
}

.bg-circle .btn.candidates {
    top: calc(50% - 40px);
    left: -70px;
    background-color: #007ba7;
}

.bg-circle .btn.candidates:hover {
    background-color: #333333;
}

.bg-circle .btn.recruiters {
    top: 80px;
    right: 0;
    background-color: #a8e1a1;
}

.bg-circle .btn.recruiters:hover {
    background-color: #007ba7;
}

.bg-circle .btn.employers {
    bottom: 10px;
    right: 112px;
    background-color: #333333;
}

.bg-circle .btn.employers:hover {
    background-color: #a8e1a1;
}

@keyframes mainRotate {
    0% {
        transform: rotate(0deg);
    }
    100% {
        transform: rotate(-360deg);
    }
}

@keyframes rotateInner {
    0% {
        transform: rotate(0deg);
    }
    100% {
        transform: rotate(360deg);
    }
}

.bg-circle .img {
    width: 96px;
    height: 96px;
    background-color: #ccc;
    position: absolute;
    border-radius: calc(96px / 2);
    animation: rotateInner 40s linear infinite;
}

.bg-circle .img img {
    width: 100%;
    height: 100%;
    position: absolute;
}

.bg-circle .img.one {
    left: calc(50% - 48px);
    top: -48px;
}

.bg-circle .img.two {
    left: 6px;
    bottom: 84px;
}

.bg-circle .img.three {
    right: -18px;
    bottom: 124px;
}

.bg-circle .dot {
    width: 24px;
    height: 24px;
    background-color: #ffffff;
    position: absolute;
    border-radius: calc(24px / 2);
}

.bg-circle .dot.one {
    left: 72px;
    top: 80px;
    border: 3px solid #007ba7;
}

.bg-circle .dot.two {
    left: 172px;
    bottom: 14px;
    border: 3px solid #a8e1a1;
}

.bg-circle .dot.three {
    right: -4px;
    top: 44%;
    border: 3px solid #333333;
}

.bg-circle .alldot {
    width: 136px;
    height: 24px;
    position: absolute;
    border-radius: calc(24px / 2);
    z-index: 0;
    animation: rotateInner 40s linear infinite;
}

.bg-circle .alldot img {
    width: 100%;
}

.bg-circle .alldot.one {
    top: 220px;
    left: -148px;
}

.bg-circle .alldot.two {
    top: 126px;
    right: -114px;
}

.bg-circle .alldot.three {
    bottom: -6px;
    right: 0px;
}

.three-inner {
    cursor: pointer;
    -webkit-transition: all 0.2s ease;
    -moz-transition: all 0.2s ease;
    -ms-transition: all 0.2s ease;
    -o-transition: all 0.2s ease;
    transition: all 0.2s ease;
}

#candidate {
    transform: translate(0px, 1px);
}


/*====================sign-up/ sign-in==========================*/

.sign-up .banner,
.sign-in .banner {
